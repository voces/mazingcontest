{
  "name": "mazingcontest",
  "version": "1.0.0",
  "description": "The WC3 game (and mazer.io) build using webcraft",
  "scripts": {
    "build": "node scripts/build.js",
    "dev": "node scripts/build.js --watch",
    "jest": "jest --silent",
    "lint": "eslint src --ext .js,.ts,.jsx,.tsx --quiet --fix",
    "start": "node --experimental-modules --no-warnings server/src/index.js",
    "test-debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test-lint": "eslint src --ext .js,.ts,.jsx,.tsx --max-warnings 0",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "license": "ISC",
  "author": "verit",
  "main": "public/server.cjs",
  "files": ["public/*"],
  "devDependencies": {
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^26.0.19",
    "@types/jsdom": "^16.2.5",
    "@types/lodash-es": "^4.17.4",
    "@types/marked": "^1.2.1",
    "@types/node": "^14.14.19",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^2.2.3",
    "esbuild": "^0.9.2",
    "eslint": "^7.16.0",
    "eslint-config-verit": "^3.4.0",
    "eslint-plugin-prettier": "^3.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "jest": "^26.6.3",
    "jest-ts-webcompat-resolver": "^1.0.0",
    "jsdom": "^16.4.0",
    "prettier": "^2.2.1",
    "three": "^0.126.1",
    "ts-jest": "^26.4.4",
    "typescript": "^4.2.2",
    "webcraft": "^3.0.0"
  },
  "type": "module"
}
